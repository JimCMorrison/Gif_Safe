<div class="showGif">
  <a href="#" class="thumbnail">
    <img src="<%= @gif.url %>">
  </a>
  <h3><%= @gif.tag %></h3>
  <p><%= @gif.url %></p>
  <p><%= @gif.description %></p>
</div>
<%= link_to 'Edit', edit_gif_path(@gif), class: "btn btn-info" %>
<%= link_to 'Delete', gif_path(@gif), class: "btn btn-danger", method: :delete, data: {confirm: "Are you sure?"} %>
<%= link_to "All GIFs", gifs_path, class: "btn btn-primary" %>
