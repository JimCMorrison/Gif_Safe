<div class="gifsIndexWrapper">
  <%= link_to "Add A GIF", new_gif_path, id: "add-gif", class: "btn btn-success" %>
  <div class="row">
    <% @gifs.each do |gif| %>
    <div class="col-md-4">
      <a href="#" class="thumbnail">
        <img src="<%= gif.url %>">
      </a>
      <h3><%= link_to gif.tag, gif %></h3>
      <p><%= gif.url %></p>
      <p><%= time_ago_in_words(gif.created_at) %></p>
    </div>
    <% end %>
  </div>
</div>
